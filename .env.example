# GitHub Personal Access Token for git operations (PRs, clones, etc)
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Anthropic API Key for Claude SDK (sub-agents, programmatic API access)
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Tailscale auth key (optional - can do interactive auth instead)
# Get from: https://login.tailscale.com/admin/settings/keys
TAILSCALE_AUTHKEY=tskey-auth-xxxxxxxxxxxx

# Git identity (use your GitHub email for Vercel deploys to work)
GIT_EMAIL=your-email@example.com
GIT_NAME=Your Name

# Proxy & Network Settings (if needed)
HTTP_PROXY=http://your-proxy-url:8080
HTTPS_PROXY=http://your-proxy-url:8080
NO_PROXY=localhost,127.0.0.1
# IP or hostname of a Tailscale node to use as a gateway (bypass firewalls)
TAILSCALE_EXIT_NODE=
# TAILSCALE_EXIT_NODE_ONLY=api.anthropic.com,claude.ai

# Agent Resource Configuration
# AGENT_CPUS=8.0
# AGENT_MEMORY=8G
# AGENT_RESERVATION_CPUS=1.0
# AGENT_RESERVATION_MEMORY=512M
# AGENT_NODE_MEMORY=6144

# Claude Code Configuration
# Max output tokens (default: 16000, max: 32000)
CLAUDE_CODE_MAX_OUTPUT_TOKENS=32000
# Check for updates on container startup (default: true)
CLAUDE_STARTUP_UPDATE=true
# Disable built-in auto-updater - prevents "Auto-update failed" errors (default: 1)
DISABLE_AUTOUPDATER=1

# Push Notifications (ntfy.sh)
# Get notified when Claude needs your input
# 1. Install ntfy app on phone (Android/iOS)
# 2. Subscribe to a unique topic name
# 3. Set NTFY_ENABLED=true and NTFY_TOPIC below
NTFY_ENABLED=false
NTFY_TOPIC=my-claude-agent-abc123
NTFY_SERVER=https://ntfy.sh
NTFY_RATE_LIMIT=15

# Webtmux (browser-based terminal)
# Disabled by default to save CPU
WEBTMUX_ENABLED=false

# Clawdbot (Telegram/Multi-Platform AI Assistant)
# 1. Create a bot with @BotFather on Telegram
# 2. Copy the bot token below
# 3. Set CLAWDBOT_ENABLED=true
CLAWDBOT_ENABLED=false
TELEGRAM_BOT_TOKEN=
TELEGRAM_GROUP_IDS=
# Brave Search API key for web search (free tier available)
# Get one at: https://brave.com/search/api/
BRAVE_API_KEY=
# Instance identity — set these for multi-instance setups
# Default: agent-mobile (main instance). Override for specialized instances.
# Example: AGENT_NAME=agent-mobile-markets
AGENT_NAME=agent-mobile
AGENT_SSH_PORT=2222
AGENT_WEB_PORT=9090
AGENT_RTS_PORT=9091
AGENT_CLAWDBOT_PORT=18789

# Clawd soul branch — controls which personality/specialization to load
# Default: master (main personality). Set to a branch name for specialized instances.
# Example: CLAWD_SOUL_BRANCH=markets (loads trading-focused soul from 'markets' branch)
CLAWD_SOUL_BRANCH=

# Gemini API key for Clawdbot AI features (memory, skills like nano-banana-pro image generation)
# Get one at: https://aistudio.google.com/apikey
GEMINI_API_KEY=

# Vercel CLI Token
# Get one at: https://vercel.com/account/tokens
VERCEL_TOKEN=

# Supabase CLI Access Token
# Get one at: https://supabase.com/dashboard/account/tokens
SUPABASE_ACCESS_TOKEN=

# Railway CLI Token
# Get one at: https://railway.app/account/tokens
RAILWAY_TOKEN=

# Per-project Supabase service role keys (add as many as needed)
# Format: SUPABASE_SERVICE_ROLE_KEY_<PROJECT>=<key>
# Get from: Supabase project settings > API
# SUPABASE_SERVICE_ROLE_KEY_MYPROJECT=eyJhbGciOi...